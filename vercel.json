{
  "buildCommand": "python3 -m pipenv run pygbag --build main.py",
  "outputDirectory": "build/web",
  "devCommand": "python3 -m pipenv run pygbag main.py",
  "installCommand": "pip install pipenv && python3 -m pipenv install --deploy",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}

